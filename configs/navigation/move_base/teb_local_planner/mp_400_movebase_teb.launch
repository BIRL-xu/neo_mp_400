<?xml version="1.0"?>
<launch>
  
 <include file="$(find neo_mp_400)/configs/navigation/amcl/mp_400_amcl.launch"/>

 <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

    <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS"/>
    <param name="controller_frequency" value="6.0"/>
    <param name="/use_sim_time" value="true"/>

    <rosparam file="$(find neo_mp_400)/configs/navigation/move_base/costmaps/$(env ROBOT)_costmap_common.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find neo_mp_400)/configs/navigation/move_base/costmaps/$(env ROBOT)_costmap_common.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find neo_mp_400)/configs/navigation/move_base/costmaps/$(env ROBOT)_costmap_local.yaml" command="load" />
    <rosparam file="$(find neo_mp_400)/configs/navigation/move_base/costmaps/$(env ROBOT)_costmap_global.yaml" command="load" />
    <rosparam file="$(find neo_mp_400)/configs/navigation/move_base/teb_local_planner/$(env ROBOT)_planner_teb.yaml" command="load" />
  </node>

</launch>
